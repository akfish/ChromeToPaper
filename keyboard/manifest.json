{
  "name": "ChromeToPaper Keyboard Shortcuts",
  "version": "1.4",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
	"options_page":"options.html",
	"background_page":"background.html",
	"content_scripts": 
	[
		{
			"matches":[
			  "http://*/*",
